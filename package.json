{
  "name": "shop_counter",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild:api": "rimraf dist",
    "prebuild:email": "rimraf dist",
    "prebuild:algolia": "rimraf dist",
    "build:api": "nest build",
    "build:email": "nest build email-subscription",
    "build:algolia": "nest build algolia-configuration",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:email": "npm run start:dev email-subscription",
    "start:algolia": "npm run start:dev algolia-configuration",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./apps/backend/test/jest-e2e.json",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js -d ./src/config/typeorm.config.ts",
    "migration:create": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli migration:create",
    "migration:generate": "npm run build:api && npm run typeorm -- migration:generate",
    "migration:run": "npm run build:api && npm run typeorm -- migration:run",
    "migration:revert": "npm run typeorm -- migration:revert",
    "compodoc": "npx @compodoc/compodoc -p tsconfig.json -s -w",
    "compodoc:backend": "npx @compodoc/compodoc -p ./apps/backend/tsconfig.app.json -s -w",
    "compodoc:email": "npx @compodoc/compodoc -p ./apps/email-subscription/tsconfig.app.json -s -w",
    "compodoc:algolia": "npx @compodoc/compodoc -p ./apps/algolia-configuration/tsconfig.app.json -s -w"
  },
  "dependencies": {
    "@nestjs/common": "^8.0.0",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^8.0.0",
    "@nestjs/jwt": "^9.0.0",
    "@nestjs/passport": "^9.0.0",
    "@nestjs/platform-express": "^8.0.0",
    "@nestjs/schedule": "^2.1.0",
    "@nestjs/swagger": "^6.0.5",
    "@nestjs/typeorm": "^9.0.1",
    "@paranode/typeorm-seeding": "^3.1.1",
    "@sendgrid/mail": "^7.7.0",
    "@types/passport": "^1.0.9",
    "algoliasearch": "^4.14.2",
    "aws-sdk": "^2.1200.0",
    "bcrypt": "^5.0.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "dotenv": "^16.0.1",
    "log4js": "^6.7.0",
    "moment": "^2.29.4",
    "mysql2": "^2.3.3",
    "nanoid": "^3.0.0",
    "nestjs-algoliasearch": "^1.0.0",
    "nestjs-typeorm-paginate": "^4.0.1",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "sharp": "^0.31.0",
    "twilio": "^3.79.0",
    "typeorm": "^0.3.7",
    "uuid": "^8.3.2",
    "winston": "^3.8.1",
    "winston-logzio": "^5.1.6"
  },
  "devDependencies": {
    "@compodoc/compodoc": "^1.1.19",
    "@nestjs/cli": "^8.0.0",
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/testing": "^8.0.0",
    "@types/bcrypt": "^5.0.0",
    "@types/cron": "^2.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "27.5.0",
    "@types/multer": "^1.4.7",
    "@types/node": "^16.0.0",
    "@types/passport-jwt": "^3.0.6",
    "@types/passport-local": "^1.0.34",
    "@types/sharp": "^0.30.5",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "28.0.3",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "28.0.1",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.0.0",
    "typescript": "^4.3.5"
  },
  "engines": {
    "node": "14.18.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/apps/"
    ]
  }
}
